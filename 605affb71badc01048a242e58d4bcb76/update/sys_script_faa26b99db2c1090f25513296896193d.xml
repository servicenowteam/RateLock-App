        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
  var first_param = '';
  var meta = {
    user: gs.getUser()
  };
  if (!current.caller_id.nil()) {
    first_param += current.getValue('caller_id');
  }

  if (!current.email_recipients.nil()) {
    if (!current.caller_id.nil()) {
      first_param += ',';
    }
    first_param += current.getValue('email_recipients');
  }
  var gdt = new GlideDateTime();
  current.resolved = gdt.getDisplayValue();
  current.active = false;
  current.update();

  meta.header_text = gs.getUserDisplayName() + ' has resolved your ratelock request.';
  gs.eventQueue('x_momo_ratelock.resolved', current, first_param, JSON.stringify(meta));


