<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_angular_provider">
    <sp_angular_provider action="DELETE">
        <name>ratelockListSessionTicker</name>
        <script><![CDATA[function(){
  return {
    template: '<div class="ticker"' +
      'ng-show="s.sys_id === sys_id && s.status !== \'exiting\'"' +
      'ng-repeat="s in sessions track by $index">' +
      '<span uib-tooltip="{{s.user_display_name}} is {{s.status}}"' +
      'ng-class="getHTMLClass(s.status)"' + 
      'tooltip-placement="top"' +
      'tooltip-append-to-body="true"></span>' +
    '</div>',
    restrict: 'E',
    replace: true,
    scope: {
      table: "=",
      ticket: "="
    },
    link: function(scope, element, attrs){},
    controller: function($scope, $rootScope, ratelockSession) {
      var c = this;
			c.$onInit = function() {
				ratelockSession.subscribe($scope.table, $scope.ticket);
			}
			
			$rootScope.$on('ratelock.sessions', function(e, payload){
				$scope.sessions = payload;
			})
      
      $scope.getHTMLClass = function(status) {
        var cls = "transparent";
          switch (status) {
            case 'exiting':
              color = 'yellow';
              break;
            case 'viewing':
              color = 'green';
              break;
            case 'typing':
              color = 'red';
              break;
            default:
              color = 'transparent';
              break;
          }
          return color;
      }
    }
  }
}]]></script>
