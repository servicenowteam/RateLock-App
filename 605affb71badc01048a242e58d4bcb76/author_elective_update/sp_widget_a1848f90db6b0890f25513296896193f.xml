    <sp_widget action="DELETE">
        <category>custom</category>
        <client_script><![CDATA[function ($scope, $rootScope, $window, $interval, $timeout, ratelockSession) {
  /* widget controller */
  var c = this;

  c.$onInit = function () {
    ratelockSession.subscribe($scope.data.table, $scope.data.sys_id);
  }

  $window.addEventListener('unload', function () {
    $timeout.cancel();
  })

  $scope.getColor = function (status) {
    var cls = "transparent";
    switch (status) {
      case 'exiting':
        color = 'yellow';
        break;
      case 'viewing':
        color = 'green';
        break;
      case 'typing':
        color = 'red';
        break;
      default:
        color = 'transparent';
        break;
    }
    return color;
  }



  $rootScope.$on("ratelock.sessions", function (e, sessions) {
    var keys = Object.keys(sessions);
    keys.forEach(function (key) {
      var session = sessions[key];
      if (key !== $window.NOW.session_id && session.sys_id === $scope.data.sys_id) {
        $scope.session = session;
        if ($scope.session.status === 'exiting') {
          $timeout(function () {
            $scope.session.status = null;
          }, 3000);
        }
      }
    });

  });
}]]></client_script>
        <controller_as>c</controller_as>
